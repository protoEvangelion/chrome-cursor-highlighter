{
  "name": "liferay-ui-superpowers",
  "version": "1.0.0",
  "description": "",
  "lint-staged": {
    "*.{js,json,md,scss}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "build": "npm run env:prod -- webpack",
    "dev": "npm run env:dev -- webpack --watch",
    "env:prod": "cross-env NODE_ENV=production",
    "env:dev": "cross-env NODE_ENV=development",
    "format": "prettier --ignore-path ./.prettierignore --write \"**/*.{js,json,md,scss}\"",
    "precommit": "lint-staged"
  },
  "author": "Ryan Garant",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "4.17.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "1.10.2",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.11.0"
  }
}
